<script type="text/javascript">
  let datetimeNodes = document.getElementsByClassName("datetime");
  console.log(datetimeNodes);
  for (let i = 0; i < datetimeNodes.length; i++) {
    var str = datetimeNodes[i].innerHTML;
    var str1 = str.substring(0, str.indexOf(":"));
    var str2 = str1.substring(str1.lastIndexOf(" "));
    if ((Number(str2) + 1) >= 10)
      datetimeNodes[i].innerHTML = str2 + "-" +  (Number(str2)+1);
    else
      datetimeNodes[i].innerHTML = str2 + "-0" +  (Number(str2)+1);
  }
</script>

